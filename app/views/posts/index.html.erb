
 <div class="container">
  <% @posts.each do |post| %>
  <div class="well">
        <div class="media-body">
          <h3 class="media-heading">
            <%=link_to post.title, post_path(post), class: 'post_title', id: "post_#{post.id}" %>
          </h3>
          <p class="text-right">
          </p>
          <p>
            <%= post.body %>
          </p>
        </div>
      </div>
  <% end %>
  </div>


<% if user_signed_in? %>
<%= link_to "Create a new post", new_post_path, class: "new_post" %>
<% end %>

